def open_spreadsheet(file_path)
  case File.extname(file_path)
  when '.csv' then
    Roo::CSV.new(file_path)
  when '.xls' then
    Roo::Excel.new(file_path)
  when '.xlsx' then
    Roo::Excelx.new(file_path)
  else
    fail "Unknown file type: #{file_path}"
  end
end
xlsx = open_spreadsheet(file)
