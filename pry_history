5.is_a? Numeric
def clean_params(params)
  params.each do |k, v|
    next if v.is_a? Numeric
    params[k] = v.split(',').reject(&:blank?).join(',') # for when commas are entered followed by nothing
    params[k].strip!; params[k].downcase!; params[k].blank?
  end
end
clean_params(params)
exit
