proxy ||= pop_proxy
Capybara.register_driver :poltergeist do |app|
  poltergeist_opts = {
    js_errors: false,
    timeout: 60,
    phantomjs_options: [
      '--ignore-ssl-errors=yes',
      "--load-images=true",
      '--web-security=false',
      "--proxy=#{proxy}",
      "--disk-cache=yes",
      "--max-disk-cache-size=200000",
      "--ssl-protocol=any",
      "--proxy-auth=#{ENV['PROXY_USERNAME']}:#{ENV['PROXY_PASSWORD']}"
    ]
  }
