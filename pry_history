def extract_keywords(tech, *keys)
  keywords = Set.new
  keys.each do |key|
    next if tech[key].blank?
    tech[key] = tech[key].split(',')
    tech[key].map!(&:squish!)
    tech[key].uniq!
    tech[key].count.times do
      keywords << tech[key].pop
    end
  end
  keywords
end
