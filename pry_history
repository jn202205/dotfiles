ENCODING_OPTIONS = {
  :invalid => :replace, # Replace invalid byte sequences
  :undef => :replace, # Replace anything not defined in ASCII
  :replace => '', # Use a blank for those replacements
  :universal_newline => true # Always break lines with \n
}
