def extract_keywords(keywords, key, t)
  return if t[key].blank?
  t[key] = t[key].split(',')
  t[key].map!(&:squish!)
  t[key].uniq!
  t[key].count.times do
    keywords << { technology_id: t['technology_id'], keyword: t[key].shift }
  end
end
