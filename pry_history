clean_urls(urls)
def clean_urls(url_csv)
  url_csv.encode(Encoding.find('ASCII'), ENCODING_OPTIONS).gsub(' ', '')
  .split(/,(?=(http|\/\/))/).map { |url| url.gsub(/(https?:)?(\/\/)?/, '') }
  .reject(&:blank?).map { |url| "http://#{url}" }
end
clean_urls(urls)
def clean_urls(url_csv)
  url_csv.encode(Encoding.find('ASCII'), ENCODING_OPTIONS).gsub(' ', '')
  .split(/,(?=(http:|\/\/))/).map { |url| url.gsub(/(https?:)?(\/\/)?/, '') }
  .reject(&:blank?).map { |url| "http://#{url}" }
end
