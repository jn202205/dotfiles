  puts "Ratio: #{p}/#{op} Total: #{total}"
  puts "#{((p.to_f / total) * 100).round(2)} percent Products"
  puts "Time: #{time.round(2)}mins Rate: #{rate.round(2)} created: #{s.created_at.strftime("%m/%d/%Y")}"
