def generate_key_mapping(header_row)
  header_row.each.with_index.each_with_object({}) do |e, a|
    key_sym = e.first.downcase.gsub(' ', '_').to_sym
    key = @keys.include? key_sym ? key_sym : e.first.to_sym
    a[key] = e.last
  end
end
