l.identifiers
def serialize_data(obj, *keys)
  key = keys.map(&:to_sym).find { |k| obj.key?(k) } if obj.respond_to?(:key?)
  if key.present?
    obj[key]
  elsif obj.respond_to?(:each)
    r = nil
    obj.find { |*a| r = serialize_data(a.last, *keys) if a.last }
    r
  end
end
